package clientes;

import entities.Usuarios;

/**
 *
 * @author misa9312
 */
public class Auxiliar 
{
    public Usuarios getUser(String username)
    {
        UsuariosClient cliente = new UsuariosClient();
        Usuarios usuario = cliente.findByUsername(Usuarios.class, username);
        return usuario;
    }
    
}
